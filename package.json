{
  "name": "ice-cream-shop",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "node dist/server.js",
    "prebuild": "tslint -c tslint.json -p tsconfig.json --fix",
    "build": "tsc --project .",
    "build-prod": "cd ics-client && npm install && npm run build && cd .. && npm install && npm run build",
    "server": "nodemon  --verbose --watch --experimental-modules\"server\" --ext \"ts,json\" --exec ts-node server/server.ts",
    "ics-client": "cd ics-client && npm start && cd ..",
    "dev": "concurrently \"npm run ics-client\" \"npm run server\""
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.24.0",
    "cookie-parser": "^1.4.5",
    "express": "^4.17.1",
    "mongodb": "^4.1.4"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.13",
    "@types/node": "^16.11.6",
    "concurrently": "^6.3.0",
    "nodemon": "^2.0.14",
    "ts-node": "^10.4.0",
    "tslint": "^6.1.3",
    "typescript": "^4.4.4"
  }
}
